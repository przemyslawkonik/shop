<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>

<AnchorPane maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controllers.SavedBasketViewController">
   <children>
      <VBox maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
         <children>
            <TableView fx:id="savedBasketTableView" prefHeight="200.0" prefWidth="200.0">
              <columns>
                <TableColumn fx:id="nameColumn" maxWidth="1.7976931348623157E308" minWidth="120.0" prefWidth="-1.0" text="Nazwa" />
                <TableColumn fx:id="quantityColumn" maxWidth="1.7976931348623157E308" minWidth="120.0" prefWidth="-1.0" text="Liczba produktów" />
                  <TableColumn fx:id="valueColumn" maxWidth="1.7976931348623157E308" minWidth="120.0" prefWidth="-1.0" text="Wartość (zł)" />
              </columns>
            </TableView>
            <HBox alignment="CENTER" spacing="15.0">
               <children>
                  <Button mnemonicParsing="false" text="Przenieś do koszyka" />
                  <Button mnemonicParsing="false" text="Usuń" />
               </children>
               <padding>
                  <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
               </padding>
            </HBox>
         </children>
      </VBox>
   </children>
</AnchorPane>
